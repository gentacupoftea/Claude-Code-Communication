# Conea プロジェクト ステージング環境変数設定
NODE_ENV=staging
REACT_APP_ENV=staging

# データベース接続設定
DB_HOST=staging-db.conea.internal
DB_USER=conea_staging_user
DB_PASSWORD=staging_db_password_placeholder
DB_NAME=conea_staging
DB_PORT=5432
DB_CONNECTION_STRING=postgresql://conea_staging_user:staging_db_password_placeholder@staging-db.conea.internal:5432/conea_staging

# Redis設定
REDIS_HOST=staging-redis.conea.internal
REDIS_PORT=6379
REDIS_PASSWORD=staging_redis_password_placeholder
REDIS_URL=redis://:staging_redis_password_placeholder@staging-redis.conea.internal:6379/0

# APIキー設定（テスト用）
API_KEY_OPENAI=sk-test-openai-key-for-staging
API_KEY_ANTHROPIC=sk-test-anthropic-key-for-staging
API_KEY_GEMINI=test-gemini-key-for-staging

# フロントエンド設定
REACT_APP_API_URL=https://staging-api.conea.shopify-mcp.com
REACT_APP_WEBSOCKET_URL=wss://staging-api.conea.shopify-mcp.com/ws
REACT_APP_OAUTH_CALLBACK_URL=https://staging.conea.shopify-mcp.com/oauth/callback
REACT_APP_ENABLE_DEBUG=true
REACT_APP_PERFORMANCE_MONITORING=true

# 認証設定
JWT_SECRET=staging_jwt_secret_placeholder
JWT_EXPIRY=8h
SESSION_SECRET=staging_session_secret_placeholder
ADMIN_PASSWORD=staging_admin_password_placeholder
COOKIE_SECURE=true
COOKIE_HTTP_ONLY=true

# ログ設定
LOG_LEVEL=info
MCP_LOG_LEVEL=INFO

# ドメイン設定
DOMAIN_NAME=staging.conea.ai
API_DOMAIN_NAME=staging-api.conea.ai
EXPECTED_IP=34.120.0.79

# Shopify統合
SHOPIFY_APP_URL=https://staging.conea.shopify-mcp.com
SHOPIFY_API_KEY=staging_shopify_api_key_placeholder
SHOPIFY_API_SECRET=staging_shopify_api_secret_placeholder
SHOPIFY_SCOPES=read_products,write_products,read_orders,read_customers

# テスト用認証情報
TEST_USERNAME=test-user
TEST_PASSWORD=test-password
TEST_API_KEY=test-api-key-for-staging

# コンテナ設定
MEMORY=2Gi
CPU=2
MIN_INSTANCES=1
MAX_INSTANCES=3

# 分析
ENABLE_ANALYTICS=true
ANALYTICS_ENDPOINT=https://analytics.conea.shopify-mcp.com/collect

# レガシー互換性（旧パッケージ名）
SHOPIFY_MCP_PROJECT_ID=conea-staging
SHOPIFY_MCP_SERVICE_NAME=conea-staging
SHOPIFY_MCP_SERVICE_ACCOUNT=conea-sa@conea-staging.iam.gserviceaccount.com