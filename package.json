{
  "name": "conea-ai-platform",
  "version": "1.1.0",
  "description": "Conea AI推薦システム - 2025年完全知能化プラットフォーム",
  "main": "index.js",
  "scripts": {
    "start": "node src/main.py",
    "dev": "python -m uvicorn src.main:app --reload --host 0.0.0.0 --port 8000",
    "test": "python -m pytest tests/ -v",
    "test:ai": "python -m pytest tests/ai/ -v --cov=src/ai",
    "test:frontend": "cd frontend && npm test",
    "test:e2e": "playwright test",
    "test:accessibility": "cd frontend && npm run test:a11y",
    "test:all": "npm run test && npm run test:frontend && npm run test:accessibility",
    "lint": "flake8 src/ && cd frontend && npm run lint",
    "lint:fix": "black src/ && cd frontend && npm run lint:fix",
    "build": "cd frontend && npm run build",
    "build:staging": "./scripts/build-staging.sh",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "docker:build": "docker build -t conea:latest .",
    "docker:run": "docker run -p 8000:8000 conea:latest",
    "migration": "python -m alembic upgrade head",
    "migration:create": "python -m alembic revision --autogenerate -m",
    "docs": "cd docs && make html",
    "security:scan": "python scripts/security_scan.py",
    "performance:test": "python scripts/performance_test.py",
    "ai:train": "python scripts/ai_training.py",
    "ai:evaluate": "python scripts/ai_evaluation.py"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "recommendation-system",
    "environment-variables",
    "accessibility",
    "wcag",
    "2025",
    "intelligence"
  ],
  "author": "Conea Development Team",
  "license": "MIT",
  "dependencies": {
    "fastapi": "^0.104.1",
    "uvicorn": "^0.24.0",
    "pydantic": "^2.4.2",
    "sqlalchemy": "^2.0.23",
    "alembic": "^1.12.1",
    "redis": "^5.0.1",
    "numpy": "^1.26.1",
    "pandas": "^2.1.3",
    "scikit-learn": "^1.3.2",
    "tensorflow": "^2.13.0",
    "torch": "^2.0.1"
  },
  "devDependencies": {
    "pytest": "^7.4.3",
    "pytest-asyncio": "^0.21.1",
    "pytest-cov": "^4.1.0",
    "black": "^23.11.0",
    "flake8": "^6.1.0",
    "mypy": "^1.7.0",
    "playwright": "^1.40.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conea-ai/conea-platform.git"
  },
  "bugs": {
    "url": "https://github.com/conea-ai/conea-platform/issues"
  },
  "homepage": "https://conea.ai"
}
