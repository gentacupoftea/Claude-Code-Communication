{
  "name": "conea-ai-platform",
  "version": "1.1.0",
  "description": "Conea AI推薦システム - 2025年完全知能化プラットフォーム",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:benchmark": "ts-node src/tests/benchmark.ts",
    "test:benchmark:baseline": "npm run test:benchmark -- --baseline",
    "test:benchmark:improved": "npm run test:benchmark -- --improved",
    "optimize:prompts": "ts-node src/optimization/prompt-optimizer.ts",
    "optimize:routing": "ts-node src/optimization/routing-optimizer.ts",
    "compare:results": "ts-node src/tests/compare-results.ts",
    "analyze:failures": "ts-node src/tests/analyze-failures.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t conea-multillm .",
    "docker:run": "docker run -p 3000:3000 --env-file .env conea-multillm",
    "docker:dev": "docker build -t conea-multillm . && docker run -p 3000:3000 --env-file .env conea-multillm"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "recommendation-system",
    "environment-variables",
    "accessibility",
    "wcag",
    "2025",
    "intelligence"
  ],
  "author": "Conea Development Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google-cloud/aiplatform": "^3.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "openai": "^4.0.0",
    "pg": "^8.16.0",
    "prom-client": "^15.1.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conea-ai/conea-platform.git"
  },
  "bugs": {
    "url": "https://github.com/conea-ai/conea-platform/issues"
  },
  "homepage": "https://conea.ai",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  }
}
