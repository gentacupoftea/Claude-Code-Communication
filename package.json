{
  "name": "conea-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:analyze": "ANALYZE=true npm run build",
    "build:cache": "next build --experimental-build-cache",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:smoke": "cypress run --spec 'cypress/e2e/auth.cy.ts,cypress/e2e/dashboard.cy.ts'",
    "test:e2e:auth": "cypress run --spec 'cypress/e2e/auth.cy.ts,cypress/e2e/registration.cy.ts'",
    "test:e2e:dashboard": "cypress run --spec 'cypress/e2e/dashboard.cy.ts'",
    "test:e2e:multillm": "cypress run --spec 'cypress/e2e/multillm-chat.cy.ts'",
    "test:e2e:projects": "cypress run --spec 'cypress/e2e/projects.cy.ts'",
    "test:e2e:api": "cypress run --spec 'cypress/e2e/api-settings.cy.ts'",
    "test:e2e:cache": "cypress run --spec 'cypress/e2e/cache.cy.ts'",
    "test:e2e:accessibility": "cypress run --spec 'cypress/e2e/accessibility.cy.ts'",
    "test:e2e:performance": "cypress run --spec 'cypress/e2e/performance.cy.ts'",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:comprehensive": "cypress run --spec 'cypress/e2e/**/*.cy.ts' --browser chrome",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.53.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "typeorm": "^0.3.24",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@jest/environment": "^30.0.0-beta.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chart.js": "^2.9.41",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.23",
    "cypress": "^13.6.2",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
