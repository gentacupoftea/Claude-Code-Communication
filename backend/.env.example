# Google Analytics設定
GA4_PROPERTY_ID=your-ga4-property-id
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour-private-key\n-----END PRIVATE KEY-----"
SEARCH_CONSOLE_SITE_URL=https://your-website.com

# スマレジ設定
SMAREGI_API_BASE_URL=https://api.smaregi.jp
SMAREGI_CONTRACT_ID=your-contract-id
SMAREGI_ACCESS_TOKEN=your-access-token
SMAREGI_CLIENT_ID=your-client-id
SMAREGI_CLIENT_SECRET=your-client-secret

# AI/LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# AI Model Configuration
OPENAI_MODEL=gpt-3.5-turbo
ANTHROPIC_MODEL=claude-3-haiku-20240307
GOOGLE_MODEL=gemini-1.5-flash
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=1000

# Enable/Disable AI Providers (optional)
ENABLE_OPENAI=true
ENABLE_CLAUDE=true
ENABLE_GEMINI=true

# Slack Bot設定（オプション）
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_SIGNING_SECRET=your-signing-secret
SLACK_APP_TOKEN=xapp-your-app-token

# GitHub設定（オプション）
GITHUB_TOKEN=your-github-token
GITHUB_WEBHOOK_SECRET=your-webhook-secret
# BigQuery設定
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=./credentials/bigquery-key.json
