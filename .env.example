# MongoDB認証情報
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=password

# Redis認証情報
REDIS_PASSWORD=redis_password

# JWT認証用シークレット
JWT_SECRET=jwt_secret_key_change_this_in_production

# Shopify API認証情報
SHOPIFY_SHOP_NAME=your-shop-name
