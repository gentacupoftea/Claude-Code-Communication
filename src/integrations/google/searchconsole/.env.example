# Google Search Console API Credentials
# OAuth 2.0認証用（どちらかを選択）
GSC_CLIENT_ID=your_oauth_client_id
GSC_CLIENT_SECRET=your_oauth_client_secret
GSC_REDIRECT_URI=http://localhost:3000/callback

# サービスアカウント認証用（推奨）
GSC_SERVICE_ACCOUNT_PATH=/path/to/service-account-key.json

# ブランドキーワード設定（カンマ区切り）
BRAND_KEYWORDS=ブランド名,会社名,商品名

# その他の設定
GSC_MAX_RETRIES=3
GSC_RETRY_DELAY=5000
GSC_CACHE_TTL=3600

# データベース設定（PostgreSQL）
DB_HOST=localhost
DB_PORT=5432
DB_NAME=shopify_mcp
DB_USER=postgres
DB_PASSWORD=postgres

# Redis設定（キャッシュ用）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0