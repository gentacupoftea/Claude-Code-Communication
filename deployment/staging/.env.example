# ビルドとデプロイの設定
BUILD_VERSION=staging

# バックエンドの設定
MONGO_URI=mongodb://mongo:27017/conea
REDIS_URL=redis://redis:6379
JWT_SECRET=changeMeInProduction
PORT=8080
NODE_ENV=production
LOG_LEVEL=info
CORS_ORIGIN=https://staging.conea.example.com

# フロントエンドの設定
REACT_APP_API_URL=https://staging.conea.example.com/api
REACT_APP_WS_URL=wss://staging.conea.example.com/ws

# Docker Composeの設定
COMPOSE_PROJECT_NAME=conea-staging