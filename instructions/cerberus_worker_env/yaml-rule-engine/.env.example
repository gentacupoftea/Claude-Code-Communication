# API Configuration
PRIMARY_API_URL=https://api.primary.com
PRIMARY_API_KEY=your-primary-api-key
SECONDARY_API_URL=https://api.secondary.com
SECONDARY_API_KEY=your-secondary-api-key

# Redis Configuration
REDIS_URL=redis://localhost:6379

# LLM Configuration
LLM_PROVIDER=ollama  # Options: openai, anthropic, ollama
LLM_API_KEY=your-llm-api-key
LLM_MODEL=llama2  # or gpt-3.5-turbo, claude-3-haiku-20240307
LLM_BASE_URL=http://localhost:11434/v1  # For Ollama

# Fallback Configuration
FALLBACK_FILE_PATH=./data/fallback-defaults.json

# Application Settings
NODE_ENV=development
LOG_LEVEL=info
LOG_DIR=./logs
PORT=3456

# Client Configuration
CLIENT_ID=fallback-system-v1